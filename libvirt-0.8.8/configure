    conftest$ac_exeext conftest.$ac_ext
   echo 'int x;' >conftest.c
   $CC $CFLAGS -c conftest.c 2>conftest.err
   ret=$?
   if test $ret != 0 || test -s conftest.err || test $has_option = "no"; then
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
